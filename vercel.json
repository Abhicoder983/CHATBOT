{
    
        "builds": [
            {
                "src": "build_files.sh",
                "use": "@vercel/static-build"
            },
            {
                "src": "registration/wsgi.py",
                "use": "@vercel/python",
                "config": {
                    "runtime": "python3.9",
                    "installCommand": "pip install -r requirements.txt"
                }
            }
        ],
    
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "registration/wsgi.py"
        }
    ]
}
